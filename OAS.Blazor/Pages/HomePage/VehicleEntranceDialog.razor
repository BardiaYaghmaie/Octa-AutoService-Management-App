                <RadzenButton Text="ثبت فاکتور" ButtonStyle="ButtonStyle.Success" Style="width: 180px"/>

            </RadzenRow>
        </RadzenColumn>
        <RadzenColumn Size="6">
            <RadzenRow>

            </RadzenRow>
            <br/>
            <RadzenRow>
                <RadzenButton Text="ثبت فاکتور متفرقه" ButtonStyle="ButtonStyle.Info" Variant="Variant.Outlined" Style="width: 180px"/>

            </RadzenRow>
        </RadzenColumn>
    </RadzenRow>
    


</MudRTLProvider>
@code {
    public string VehicleCode { get; set; }
    
}